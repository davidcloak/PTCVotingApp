@model IEnumerable<PTCVotingWebApp.Models.Voting>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Canidate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkRace)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LocationId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Canidate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkRace)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocationId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Voting1">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Voting1">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Voting1">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
